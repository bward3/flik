  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bward3/flik/issues"
  },
  "homepage": "https://github.com/bward3/flik#readme",
  "dependencies": {
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "concurrently": "^7.5.0"
  }
}
